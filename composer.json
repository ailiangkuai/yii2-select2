{
    "name": "ailiangkuai/yii2-select2",
    "description": "Yii2 extension widget of select2",
    "type": "yii2-extension",
    "require": {
        "yiisoft/yii2": ">=2.0.6",
        "php": ">=5.5.0"
    },
    "license": "GPL2.0",
    "authors": [
        {
            "name": "Rico yao",
            "email": "ailiangkuai@qq.com"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "autoload": {
        "psr-4": {"ailiangkuai\\yii2\\widgets\\select2\\": "src/"}
    }
}
